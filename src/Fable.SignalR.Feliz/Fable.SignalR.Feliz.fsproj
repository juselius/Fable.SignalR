  </PropertyGroup>
  <ItemGroup>
    <Content Include="*.fsproj; *.fs" PackagePath="fable/"/>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Feliz" Version="2.8.0"/>
  </ItemGroup>
</Project>
